<%= bootstrap_form_for(@formroute) do |f| %>

  <%= f.text_field :name, placeholder: "Form Name", required: true, autofocus: true %>
  <%= f.label "Site Page" %><br>
  <%= f.text_field :page, placeholder: "https://example.com/contact", skip_label: true, required: true %>
  <%= f.text_field :fwd_to_email, placeholder: "support@example.com", required: true %>
  <%= f.submit "Update Form Route", class: "btn btn-default form-btn" %>

<% end %>
<br>