<%= form_for(@formroute) do |f| %>
  <% if @formroute.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@formroute.errors.count, "error") %> prohibited this formroute from being saved:</h2>

      <ul>
      <% @formroute.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <form role="form">
    <div class="field form-group">
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </div>
    <div class="field form-group">
      <%= f.label :page %><br>
      <%= f.text_field :page %>
    </div>
    <div class="field form-group">
      <%= f.label :fwd_to_email %><br>
      <%= f.text_field :fwd_to_email %>
    </div>
    <div class="actions">
      <%= f.submit :class => 'btn btn-default' %>
    </div>
  </form>
<% end %>